    @Styles.Render("~/Content/css")
    <script type="text/javascript" src="/Scripts/jquery-1.9.0.min.js"></script>
    @RenderSection("JavaScript", required: false)
</head>
<body>
<div>
    @RenderBody()
</div>
</body>
</html>